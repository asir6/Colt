<WidgetInfo>
  <Type>Scalebar</Type>
  <LocalizedType>比例尺</LocalizedType>
  <Description>当前地图的显示比例。</Description>
  <Location></Location>
  <Label>比例尺</Label>
  <Tooltip></Tooltip>
  <StatusText></StatusText>
  <ImageUrl></ImageUrl>
  <ImageClass></ImageClass>
  <StandardUi>false</StandardUi>
  <ContainableBy></ContainableBy>
  <Parameter>
    <Name>Style</Name>
    <Description>比例尺样式</Description>
    <Type>String</Type>
    <Label>样式</Label>
    <AllowedValue>
      <Name>fancy</Name>
      <Label>丰富</Label>
    </AllowedValue>
    <AllowedValue>
      <Name>fat</Name>
      <Label>标准</Label>
    </AllowedValue>
    <AllowedValue>
      <Name>thin</Name>
      <Label>精简</Label>
    </AllowedValue>
    <AllowedValue>
      <Name>thinner</Name>
      <Label>极简</Label>
    </AllowedValue>
    <DefaultValue>thin</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>DisplaySystem</Name>
    <Description>显示单位</Description>
    <Type>String</Type>
    <Label>显示单位</Label>
    <AllowedValue>
      <Name>metric</Name>
      <Label>公制</Label>
    </AllowedValue>
    <AllowedValue>
      <Name>english</Name>
      <Label>英制</Label>
    </AllowedValue>
    <DefaultValue>metric</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>MinWidth</Name>
    <Description>比例尺的最小宽度</Description>
    <Type>integer</Type>
    <Label>最小宽度</Label>
    <DefaultValue>100</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>MaxWidth</Name>
    <Description>比例尺的最大宽度</Description>
    <Type>integer</Type>
    <Label>最大宽度</Label>
    <DefaultValue>200</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>Divisions</Name>
    <Description>主分段的数量</Description>
    <Type>integer</Type>
    <Label>分段</Label>
    <DefaultValue>2</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>SubDivisions</Name>
    <Description>每个主分段中自分段的数量</Description>
    <Type>integer</Type>
    <Label>子分段</Label>
    <DefaultValue>2</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>ShowMinorMeasures</Name>
    <Description>是否显示子分段</Description>
    <Type>boolean</Type>
    <Label>显示子分段</Label>
    <DefaultValue>true</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>AbbreviateLabel</Name>
    <Description>是否以精简方式显示单位</Description>
    <Type>boolean</Type>
    <Label>精简标签</Label>
    <DefaultValue>true</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>SingleLine</Name>
    <Description>是否将比例尺显示在同一行上</Description>
    <Type>boolean</Type>
    <Label>同行显示</Label>
    <DefaultValue>false</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
</WidgetInfo>

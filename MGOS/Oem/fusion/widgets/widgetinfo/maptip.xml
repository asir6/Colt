<WidgetInfo>
  <Type>Maptip</Type>
  <LocalizedType>地图提示</LocalizedType>
  <Description>当鼠标移动到地图上时显示要素信息。</Description>
  <Location></Location>
  <Label>地图提示</Label>
  <Tooltip>点击使用/禁用地图提示</Tooltip>
  <StatusText></StatusText>
  <ImageUrl>images/maptip.png</ImageUrl>
  <ImageClass>Maptip</ImageClass>
  <StandardUi>true</StandardUi>
  <ContainableBy>Any</ContainableBy>
  <Parameter>
    <Name>Delay</Name>
    <Description>显示地图提示的延时时间(毫秒)。</Description>
    <Type>integer</Type>
    <Label>延时</Label>
    <Min>0</Min>
    <Max>10000</Max>
    <DefaultValue>350</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>Layer</Name>
    <Description>显示要素信息的图层。</Description>
    <Type>String</Type>
    <Label>图层</Label>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>Tolerance</Name>
    <Description>鼠标点选可容许的像素误差。</Description>
    <Type>integer</Type>
    <Label>容错值</Label>
    <Min>0</Min>
    <Max>10</Max>
    <DefaultValue>2</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>Target</Name>
    <Label>目标</Label>
    <Type>String</Type>
    <Description>指定显示要素信息的框架、窗口或任务面板。 默认为新窗口。</Description>
    <IsMandatory>false</IsMandatory>
    <DefaultValue>MaptipWindow</DefaultValue>
  </Parameter>
  <Parameter>
    <Name>WinFeatures</Name>
    <Label>窗口设置</Label>
    <Type>String</Type>
    <Description>新窗口的设置信息</Description>
    <IsMandatory>false</IsMandatory>
    <DefaultValue>menubar=no,location=no,resizable=no,status=no</DefaultValue>
  </Parameter>
</WidgetInfo>

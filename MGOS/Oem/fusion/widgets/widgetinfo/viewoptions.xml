<WidgetInfo>
  <Type>ViewOptions</Type>
  <LocalizedType>视图选项</LocalizedType>
  <Description>为当前视图和视图中的其他工具设定显示单位。</Description>
  <Location></Location>
  <Label>地图单位</Label>
  <Tooltip>点击切换显示单位</Tooltip>
  <StatusText></StatusText>
  <ImageUrl>images/icons.png</ImageUrl>
  <ImageClass>options</ImageClass>
  <StandardUi>true</StandardUi>
  <ContainableBy>Any</ContainableBy>
  <Parameter>
    <Name>DisplayUnits</Name>
    <Description>当前视图和视图中的其他工具中的显示单位</Description>
    <Type>String</Type>
    <Label>显示单位</Label>
    <AllowedValue>
      <Name>Miles</Name>
      <Label>英里</Label>
    </AllowedValue>
    <AllowedValue>
      <Name>Meters</Name>
      <Label>米</Label>
    </AllowedValue>
    <AllowedValue>
      <Name>Degrees</Name>
      <Label>度</Label>
    </AllowedValue>
    <DefaultValue></DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
</WidgetInfo>

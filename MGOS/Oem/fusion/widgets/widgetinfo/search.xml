<WidgetInfo>
  <Type>Search</Type>
  <LocalizedType>查找</LocalizedType>
  <Description>执行基于要素属性的查找。</Description>
  <Location></Location>
  <Label>查找</Label>
  <Tooltip>点击查找地图数据</Tooltip>
  <StatusText></StatusText>
  <ImageUrl>images/icons.png</ImageUrl>
  <ImageClass>search</ImageClass>
  <StandardUi>true</StandardUi>
  <ContainableBy>Any</ContainableBy>
  <Parameter>
    <Name>Target</Name>
    <Label>Target</Label>
    <Type>目标</Type>
    <Description>指定显示查找信息的框架、窗口或任务面板。 默认为新窗口</Description>
    <IsMandatory>true</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>Prompt</Name>
    <Description>用户输入框的提示</Description>
    <Type>String</Type>
    <Label>提示</Label>
    <DefaultValue></DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>Layer</Name>
    <Description>执行查找的图层名称</Description>
    <Type>String</Type>
    <Label>图层</Label>
    <DefaultValue></DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>Filter</Name>
    <Description>查找表达式模板</Description>
    <Type>String</Type>
    <Label>过滤</Label>
    <DefaultValue>String</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>MatchLimit</Name>
    <Description>返回查找匹配的最大数量</Description>
    <Type>integer</Type>
    <Label>匹配数量限制</Label>
    <Min>0</Min>
    <DefaultValue>100</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>ResultColumns</Name>
    <Description>查找返回结果数据列的XML结构</Description>
    <Type>XML</Type>
    <Label>显示数据列</Label>
    <DefaultValue></DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>Title</Name>
    <Description>查找窗口的标题</Description>
    <Type>String</Type>
    <Label>标题</Label>
    <DefaultValue></DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
</WidgetInfo>

<WidgetInfo>
  <Type>CursorPosition</Type>
  <LocalizedType>指针坐标</LocalizedType>
  <Description>显示鼠标指针的位置坐标。</Description>
  <Label>指针坐标</Label>
  <Tooltip>显示当前鼠标指针的位置坐标</Tooltip>
  <StandardUi>false</StandardUi>
  <ContainableBy>Splitterbar</ContainableBy>
  <Parameter>
    <Name>Template</Name>
    <Type>string</Type>
    <Label>显示形式</Label>
    <Description>显示指针位置的形式</Description>
    <DefaultValue>x: {x}, y: {y}</DefaultValue>
    <IsMandatory>1</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>Precision</Name>
    <Type>integer</Type>
    <Label>显示精度</Label>
    <Description>显示坐标的精度</Description>
    <DefaultValue>4</DefaultValue>
    <Min>0</Min>
    <Max>15</Max>
    <IsMandatory>0</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>DisplayProjection</Name>
    <Type>string</Type>
    <Label>显示投影</Label>
    <Description>显示坐标的投影坐标系。 可用EPSG代码(EPSG:XXXX)或WKT形式。若指定则覆盖当前地图坐标系单位</Description>
    <DefaultValue></DefaultValue>
    <IsMandatory>0</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>Units</Name>
    <Type>string</Type>
    <Label>显示单位</Label>
    <Description>坐标单位</Description>
    <DefaultValue>dd</DefaultValue>
    <IsMandatory>1</IsMandatory>
  </Parameter>
</WidgetInfo>

<WidgetInfo>
  <Type>SelectionInfo</Type>
  <LocalizedType>选择信息</LocalizedType>
  <Description>显示选中的要素数目和图层数目。</Description>
  <Location></Location>
  <Label>选择信息</Label>
  <Tooltip></Tooltip>
  <StatusText></StatusText>
  <ImageUrl></ImageUrl>
  <ImageClass></ImageClass>
  <StandardUi>false</StandardUi>
  <ContainableBy>Splitterbar</ContainableBy>
  <Parameter>
    <Name>Template</Name>
    <Description>输出格式</Description>
    <Type>String</Type>
    <Label>输出格式模板</Label>
    <DefaultValue>在{1}个图层中选中了{0}个要素</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>EmptyText</Name>
    <Description>未选中任何要素时显示的信息</Description>
    <Type>String</Type>
    <Label>未选中信息</Label>
    <DefaultValue>尚未选中任何要素</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
</WidgetInfo>

<WidgetInfo>
  <Type>PanOnClick</Type>
  <LocalizedType>点击平移</LocalizedType>
  <Description>鼠标单击时按照设定的方向平移当前地图。</Description>
  <Location></Location>
  <Label>点击平移</Label>
  <Tooltip>鼠标单击时按照设定的方向平移当前地图</Tooltip>
  <StatusText></StatusText>
  <ImageUrl></ImageUrl>
  <ImageClass></ImageClass>
  <StandardUi>true</StandardUi>
  <ContainableBy>Any</ContainableBy>
  <Parameter>
    <Name>Percentage</Name>
    <Description>平移地图宽/高的百分比</Description>
    <Type>integer</Type>
    <Label>平移比率</Label>
    <Min>10</Min>
    <Max>100</Max>
    <DefaultValue>75</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>Direction</Name>
    <Description>平移方向</Description>
    <Type>String</Type>
    <Label>平移方向</Label>
    <AllowedValue>
      <Name>north</Name>
      <Label>北</Label>
    </AllowedValue>
    <AllowedValue>
      <Name>south</Name>
      <Label>南</Label>
    </AllowedValue>
    <AllowedValue>
      <Name>east</Name>
      <Label>东</Label>
    </AllowedValue>
    <AllowedValue>
      <Name>west</Name>
      <Label>西</Label>
    </AllowedValue>
    <DefaultValue>none</DefaultValue>
    <IsMandatory>true</IsMandatory>
  </Parameter>
</WidgetInfo>




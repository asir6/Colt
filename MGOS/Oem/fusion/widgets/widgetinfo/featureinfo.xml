<WidgetInfo>
    <Type>FeatureInfo</Type>
    <Location></Location>
    <LocalizedType>要素信息</LocalizedType>
    <Description>显示被选中的要素信息。</Description>
    <Label>要素信息</Label>
    <Tooltip>点击显示被选中要素的信息</Tooltip>
    <ImageUrl>images/icons.png</ImageUrl>
    <ImageClass>featureinfo</ImageClass>
    <StandardUi>true</StandardUi>
    <ContainableBy>Any</ContainableBy>
    <Parameter>
        <Name>Target</Name>
        <Label>目标</Label>
        <Type>target</Type>
        <Description>指定显示要素信息界面的框架、窗口或任务面板。 默认为新窗口</Description>
        <IsMandatory>true</IsMandatory>
        <DefaultValue>TaskPane</DefaultValue>
    </Parameter>
</WidgetInfo>

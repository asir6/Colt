<WidgetInfo>
  <Type>Select</Type>
  <LocalizedType>选择</LocalizedType>
  <Description>在地图上选择要素。</Description>
  <Location></Location>
  <Label>选择</Label>
  <Tooltip>点击选择要素</Tooltip>
  <StatusText></StatusText>
  <ImageUrl>images/icons.png</ImageUrl>
  <ImageClass>select</ImageClass>
  <StandardUi>true</StandardUi>
  <ContainableBy>Any</ContainableBy>
  <Parameter>
    <Name>SelectionType</Name>
    <Description>选择要素的方式</Description>
    <Type>String</Type>
    <Label>选择方式</Label>
    <DefaultValue>INTERSECTS</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>Tolerance</Name>
    <Description>鼠标点击的容许像素误差</Description>
    <Type>integer</Type>
    <Label>容错值</Label>
    <Min>1</Min>
    <DefaultValue>3</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>QueryActiveLayer</Name>
    <Description>是否只在活动层上选择要素</Description>
    <Type>boolean</Type>
    <Label>仅在活动层选择</Label>
    <DefaultValue>false</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
</WidgetInfo>

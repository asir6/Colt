<WidgetInfo>
    <Type>Theme</Type>
    <Location></Location>
    <LocalizedType>专题图</LocalizedType>
    <Description>基于当前地图图层上生成专题图层。</Description>
    <Label>专题图</Label>
    <Tooltip>点击创建专题图</Tooltip>
    <ImageUrl>images/icons.png</ImageUrl>
    <ImageClass>theme</ImageClass>
    <StandardUi>true</StandardUi>
    <ContainableBy>Any</ContainableBy>
    <Parameter>
        <Name>Target</Name>
        <Label>目标</Label>
        <Type>target</Type>
        <Description>指定框架、窗口或任务面板作为专题图的显示界面，默认为新窗口。</Description>
        <IsMandatory>true</IsMandatory>
        <DefaultValue>TaskPane</DefaultValue>
    </Parameter>
</WidgetInfo>

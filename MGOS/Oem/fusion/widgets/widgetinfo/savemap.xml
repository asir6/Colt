<WidgetInfo>
  <Type>SaveMap</Type>
  <LocalizedType>保存地图</LocalizedType>
  <Description>将当前地图作为图片保存。</Description>
  <Label>保存地图</Label>
  <Tooltip>点击保存地图</Tooltip>
  <ImageUrl>images/icons.png</ImageUrl>
  <ImageClass>file-save</ImageClass>
  <StandardUi>true</StandardUi>
  <ContainableBy>Any</ContainableBy>
  <Parameter>
    <Name>Format</Name>
    <Description>保存地图的图片格式</Description>
    <Type>String</Type>
    <Label>图片格式</Label>
    <AllowedValue>
      <Name>dwf</Name>
      <Label>DWF</Label>
    </AllowedValue>
    <AllowedValue>
      <Name>png</Name>
      <Label>PNG</Label>
    </AllowedValue>
    <AllowedValue>
      <Name>jpg</Name>
      <Label>JPEG</Label>
    </AllowedValue>
    <AllowedValue>
      <Name>gif</Name>
      <Label>GIF</Label>
    </AllowedValue>
    <DefaultValue>png</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>Scale</Name>
    <Description>地图保存比例尺，0为当前地图比例</Description>
    <Type>integer</Type>
    <Label>比例</Label>
    <DefaultValue></DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
      <Name>ResourceId</Name>
      <Description>为创建的DWF指定打印布局。</Description>
      <Type>PrintLayout</Type>
      <Label>打印布局 (仅限DWF格式)</Label>
      <DefaultValue></DefaultValue>
      <IsMandatory>false</IsMandatory>
  </Parameter>
</WidgetInfo>

<WidgetInfo>
  <Type>ZoomToSelection</Type>
  <LocalizedType>缩放至选中要素</LocalizedType>
  <Description>缩放当前地图并显示所有被选中的要素。</Description>
  <Location></Location>
  <Label>缩放至被选中要素</Label>
  <Tooltip>点击缩放至选中要素</Tooltip>
  <StatusText></StatusText>
  <ImageUrl>images/icons.png</ImageUrl>
  <ImageClass>select-zoom</ImageClass>
  <StandardUi>true</StandardUi>
  <ContainableBy>Any</ContainableBy>
  <Parameter>
    <Name>MaximumZoomDimension</Name>
    <Description>控制最大的缩放比例</Description>
    <Type>integer</Type>
    <Label>最大缩放尺寸</Label>
    <DefaultValue>-1</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>ZoomFactor</Name>
    <Description>仍使所有要素都显示在屏幕上的缩放值</Description>
    <Type>float</Type>
    <Label>缩放因子</Label>
    <DefaultValue>2</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
</WidgetInfo>

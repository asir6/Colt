<WidgetInfo>
  <Type>Legend</Type>
  <LocalizedType>图例</LocalizedType>
  <Description>以树形结构显示当前地图中的所有图层</Description>
  <Location></Location>
  <Label>图例</Label>
  <Tooltip>地图图例</Tooltip>
  <StatusText></StatusText>
  <ImageUrl></ImageUrl>
  <ImageClass></ImageClass>
  <StandardUi>false</StandardUi>
  <ContainableBy></ContainableBy>
  <Parameter>
    <Name>HideInvisibleLayers</Name>
    <Description>在图例中隐藏不可见图层</Description>
    <Type>boolean</Type>
    <Label>隐藏不可见图层</Label>
    <DefaultValue>true</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>ShowRootFolder</Name>
    <Description>是否显示根目录</Description>
    <Type>boolean</Type>
    <Label>显示根目录</Label>
    <DefaultValue>false</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>ShowMapFolder</Name>
    <Description>是否显示地图目录</Description>
    <Type>boolean</Type>
    <Label>是否显示地图目录</Label>
    <DefaultValue>false</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>LayerRasterIcon</Name>
    <Description>栅格图层图标</Description>
    <Type>String</Type>
    <Label>栅格图层图标</Label>
    <DefaultValue>images/icons/legend-raster.png</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>LayerDWFIcon</Name>
    <Description>DWF图层图标</Description>
    <Type>String</Type>
    <Label>DWF图层图标</Label>
    <DefaultValue>images/icons/legend-DWF.png</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>LayerThemeIcon</Name>
    <Description>专题图层图标</Description>
    <Type>String</Type>
    <Label>专题图层图标</Label>
    <DefaultValue>images/icons/legend-theme.png</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>DisabledLayerIcon</Name>
    <Description>禁用图层图标</Description>
    <Type>String</Type>
    <Label>禁用图层图标</Label>
    <DefaultValue>images/icons/legend-layer.png</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>LayerInfoIcon</Name>
    <Description>图层信息图标</Description>
    <Type>String</Type>
    <Label>图层信息图标</Label>
    <DefaultValue>images/icons/tree_layer_info.png</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>GroupInfoIcon</Name>
    <Description>图层组信息图标</Description>
    <Type>String</Type>
    <Label>图层组信息图标</Label>
    <DefaultValue>images/icons/tree_group_info.png</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>RootFolderIcon</Name>
    <Description>地图根目录图标</Description>
    <Type>String</Type>
    <Label>地图根目录图标</Label>
    <DefaultValue>images/icons/legend-map.png</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
</WidgetInfo>

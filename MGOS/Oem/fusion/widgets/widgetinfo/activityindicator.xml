<WidgetInfo>
  <Type>ActivityIndicator</Type>
  <LocalizedType>加载指示</LocalizedType>
  <Description>当地图加载或重新绘制时，显示页面中的特定控件。加载或绘制完毕时，隐藏该控件。</Description>
  <Location></Location>
  <Label>加载指示</Label>
  <Tooltip>正在加载地图</Tooltip>
  <StatusText></StatusText>
  <ImageUrl>images/icon_loading.gif</ImageUrl>
  <ImageClass></ImageClass>
  <StandardUi>false</StandardUi>
  <ContainableBy>Toolbar</ContainableBy>
  <Parameter>
    <Name>ElementId</Name>
    <Description>页面中包含加载指示界面的DOM编号/Description>
    <Type>String</Type>
    <Label>DOM编号</Label>
    <IsMandatory>false</IsMandatory>
  </Parameter>
</WidgetInfo>

<WidgetInfo>
  <Type>Help</Type>
  <LocalizedType>帮助</LocalizedType>
  <Description>显示帮助页面</Description>
  <Location></Location>
  <Label>帮助</Label>
  <Tooltip>点击打开帮助页面</Tooltip>
  <StatusText></StatusText>
  <ImageUrl>images/icons.png</ImageUrl>
  <ImageClass>help</ImageClass>
  <StandardUi>true</StandardUi>
  <ContainableBy>Any</ContainableBy>
  <Parameter>
    <Name>Target</Name>
    <Description>指定显示帮助页面的框架、窗口或任务面板。 默认为新窗口。</Description>
    <Type>String</Type>
    <Label>目标</Label>
    <DefaultValue>HelpWindow</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
  <Parameter>
    <Name>Url</Name>
    <Description>帮助页面链接地址</Description>
    <Type>String</Type>
    <Label>帮助页面URL链接</Label>
    <DefaultValue>widgets/Help/Help.html</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
</WidgetInfo>

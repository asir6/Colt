<WidgetInfo>
  <Type>ZoomOnClick</Type>
  <LocalizedType>点击缩放</LocalizedType>
  <Description>点击按照设定的值缩放当前地图。</Description>
  <Location></Location>
  <Label></Label>
  <Tooltip></Tooltip>
  <StatusText></StatusText>
  <ImageUrl></ImageUrl>
  <ImageClass></ImageClass>
  <StandardUi>true</StandardUi>
  <ContainableBy>Any</ContainableBy>
  <Parameter>
    <Name>Factor</Name>
    <Description>设置缩放值。 正值放大，负值缩小。</Description>
    <Type>double</Type>
    <Label>缩放因子</Label>
    <DefaultValue>2</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
</WidgetInfo>

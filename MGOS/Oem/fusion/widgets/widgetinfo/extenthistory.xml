<WidgetInfo>
  <Type>ExtentHistory</Type>
  <LocalizedType>视图历史</LocalizedType>
  <Description>在以往的视图历史中选择某一视图。</Description>
  <Label></Label>
  <ImageUrl></ImageUrl>
  <ImageClass></ImageClass>
  <Tooltip></Tooltip>
  <StandardUi>true</StandardUi>
  <ContainableBy>Any</ContainableBy>
  <Parameter>
    <Name>Direction</Name>
    <Description>历史视图中的选择方向</Description>
    <Type>string</Type>
    <Label>选择方向</Label>
    <AllowedValue>
      <Name>Next</Name>
      <Label>后一个</Label>
    </AllowedValue>
    <AllowedValue>
      <Name>Previous</Name>
      <Label>前一个</Label>
    </AllowedValue>
    <DefaultValue>Previous</DefaultValue>
    <IsMandatory>true</IsMandatory>
  </Parameter>
</WidgetInfo>

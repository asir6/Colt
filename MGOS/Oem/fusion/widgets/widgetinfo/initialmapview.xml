<WidgetInfo>
  <Type>InitialMapView</Type>
  <LocalizedType>初始视图</LocalizedType>
  <Description>缩放至初始视图或全显地图。</Description>
  <Location></Location>
  <Label>初始视图</Label>
  <Tooltip>点击切换至初始视图范围或全显地图</Tooltip>
  <StatusText></StatusText>
  <ImageUrl>images/icons.png</ImageUrl>
  <ImageClass>zoom-full</ImageClass>
  <StandardUi>true</StandardUi>
  <ContainableBy>Any</ContainableBy>
  <Parameter>
    <Name>ViewType</Name>
    <Description>缩放至原始视图或全显当前地图。</Description>
    <Type>String</Type>
    <Label>视图类型</Label>
    <AllowedValue>
      <Name>full</Name>
      <Label>全显地图</Label>
    </AllowedValue>
    <AllowedValue>
      <Name>initial</Name>
      <Label>初始视图</Label>
    </AllowedValue>
    <DefaultValue>initial</DefaultValue>
    <IsMandatory>false</IsMandatory>
  </Parameter>
</WidgetInfo>
